{
  "version": "1.1.0",
  "author": "Community for NL Design System",
  "description": "Angular web components for the Municipality of Tilburg based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "name": "@tilburg/component-library-angular",
  "keywords": [
    "nl-design-system"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/tilburg.git",
    "directory": "packages/component-library-angular"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run '/^build:.*/' --passWithNoTests",
    "build:components": "ng build && pnpm test-results",
    "clean": "rimraf dist/ .angular/",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:ts": "tsc --noEmit --project tsconfig.json",
    "watch:build": "ng build --watch",
    "test-results": "jest --json --outputFile=dist/.jest-test-results.json --silent --passWithNoTests",
    "test": "jest --verbose --passWithNoTests",
    "test-build": "node test-exports.mjs",
    "watch:test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --watch",
    "storybook": "ng run component-library-angular:storybook",
    "build-storybook": "ng run component-library-angular:build-storybook"
  },
  "dependencies": {
    "tslib": "2.8.0"
  },
  "peerDependencies": {
    "@utrecht/component-library-angular": "1.1.0",
    "@utrecht/design-tokens": "1.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/animations": "18.1.0",
    "@angular/cli": "18.1.0",
    "@angular/common": "18.1.0",
    "@angular/compiler": "18.1.0",
    "@angular/compiler-cli": "18.1.0",
    "@angular/core": "18.1.0",
    "@angular/forms": "18.1.0",
    "@angular/platform-browser": "18.1.0",
    "@angular/platform-browser-dynamic": "18.1.0",
    "@angular/router": "18.1.0",
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@storybook/addon-a11y": "8.3.6",
    "@storybook/addon-controls": "8.3.6",
    "@storybook/addon-docs": "8.3.6",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-interactions": "8.3.6",
    "@storybook/addon-jest": "8.3.6",
    "@storybook/addon-links": "8.3.6",
    "@storybook/addon-measure": "8.3.6",
    "@storybook/addon-onboarding": "8.3.6",
    "@storybook/addon-outline": "8.3.6",
    "@storybook/addon-toolbars": "8.3.6",
    "@storybook/addon-viewport": "8.3.6",
    "@storybook/angular": "8.3.6",
    "@storybook/blocks": "8.3.6",
    "@storybook/builder-webpack5": "8.3.6",
    "@storybook/components": "8.3.6",
    "@storybook/manager-api": "8.3.6",
    "@storybook/test": "8.3.6",
    "@storybook/types": "8.3.6",
    "@storybook/theming": "8.3.6",
    "@testing-library/angular": "17.3.1",
    "@testing-library/jest-dom": "6.6.2",
    "@tilburg/assets": "workspace:*",
    "@tilburg/design-tokens": "workspace:*",
    "@tilburg/font": "workspace:*",
    "@tilburg/storybook-helpers": "workspace:*",
    "@tilburg/component-library-angular": "workspace:*",
    "@types/node": "20.14.10",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@utrecht/component-library-angular": "1.1.0",
    "@utrecht/design-tokens": "1.0.0",
    "add": "2.0.6",
    "chokidar-cli": "3.0.0",
    "codelyzer": "6.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.6",
    "link": "2.1.1",
    "lodash.clonedeepwith": "4.5.0",
    "lodash.isequal": "4.5.0",
    "ng-packagr": "18.2.1",
    "npm-check-updates": "16.14.20",
    "npm-run-all": "4.1.5",
    "pnpm": "9.6.0",
    "prettier": "3.3.3",
    "rxjs": "7.8.1",
    "storybook": "8.3.6",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "zone.js": "0.14.0"
  },
  "module": "dist/fesm2022/tilburg-component-library-angular.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./dist/package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/tilburg-component-library-angular.mjs",
      "esm": "./dist/esm2022/tilburg-component-library-angular.mjs",
      "default": "./dist/fesm2022/tilburg-component-library-angular.mjs"
    },
    "./src/design-tokens.css": {
      "default": "./src/design-tokens.css"
    },
    "./dist/src/design-tokens.css": {
      "default": "./dist/src/design-tokens.css"
    }
  }
}
